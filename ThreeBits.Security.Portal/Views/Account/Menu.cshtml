@model IEnumerable<ThreeBits.Security.Portal.SECURITYWCF.PermisosXMenuBE>



@{

    foreach (ThreeBits.Security.Portal.SECURITYWCF.PermisosXMenuBE item in ViewBag.oListaMenu)
    {
<li class="nav-item" role="tab" id="heading-@item.NOMBREMENU">
    <a class="nav-link ripple with-arrow collapsed" data-toggle="collapse" data-target="#collapse-dashboards" href="@item.URL" aria-expanded="false" aria-controls="collapse-dashboards">
        <i class="icon s-4 icon-tile-four"></i>
        <span>@item.NOMBREMENU</span>
    </a>
    <ul id="collapse-dashboards" class='collapse ' role="tabpanel" aria-labelledby="heading-@item.NOMBREMENU" data-children=".nav-item">

        @{
            ((ThreeBits.Security.Portal.Controllers.AccountController)this.ViewContext.Controller).get_sumbenu(Convert.ToInt32(item.IDPERMISOSMENU.ToString()));
        }

        @foreach (ThreeBits.Security.Portal.SECURITYWCF.PermisoXSubmenuBE iSubmenu in Session["SubMenu"] as List<ThreeBits.Security.Portal.SECURITYWCF.PermisoXSubmenuBE>)
        {
            <li class="nav-item">
                <a class="nav-link ripple " href="@iSubmenu.URL" data-url="user-interface-page-layouts-blank.html">
                    <span>@iSubmenu.NOMBRESUBMENU</span>
                </a>
            </li>
        }
    </ul>
</li>

    }




}















